      "LaunchType": "FARGATE",
      "NetworkConfiguration": {
        "awsvpcConfiguration": {
          "Subnets": ["<SUBNET_1>", "<SUBNET_2>"],
          "SecurityGroups": ["<SECURITY_GROUP>"],
          "AssignPublicIp": "ENABLED"
        }
      },
      "PlatformVersion": "LATEST"
    }
  }
]
